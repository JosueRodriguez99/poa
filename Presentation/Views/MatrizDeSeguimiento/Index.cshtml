@{
    ViewBag.Title = "Matriz de Seguimiento";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-sm-3">
            <div class="form-group">
                <label>Mes:</label> <select class="form-control" name="Mes"></select>
            </div>
    </div>
</div>

<table class="table table-hover table-bordered">
    <thead>
        <tr><th>Código Producto</th><th>No.</th><th>Actividad/Tarea</th><th>Indicador</th>
        <th>Programado</th><th>Ejecutado</th><th>Cumplimiento</th><th>Cumplimiento-Indicador</th></tr>
    </thead>
    <tbody>  
    </tbody>
</table>

<!-- Botones de control -->
<div class="row">
    <div class="col-sm-12">
         <a href="#">
            <button type="button" class="btn btn-default">Guardar</button>
        </a>
         <a href="~/MatrizDeSeguimiento/Create">
            <button type="button" class="btn btn-default">Enviar</button>
        </a>
        <div class="btn-toolbar pull-right" role="toolbar">
            <div class="btn-group">
                <button type="button" class="btn btn-default">Anterior</button>
                <button type="button" class="btn btn-default">Siguiente</button>
            </div>
        </div>
    </div>
</div>
